<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<!-- Put site-specific property overrides in this file. -->

<configuration>

    <property>
		<name>rabbit.home.dir</name>
		<value>hdfs://nn:9000/rabbit</value>
		<description>Rabbit's home dir,put rabbit files in</description>
    </property>
    
     <property>
		<name>rabbit.tmp.dir</name>
		<value>tmp</value>
		<final>true</final>
		<description>Base on Rabbit home dir</description>
    </property>
    
</configuration>
